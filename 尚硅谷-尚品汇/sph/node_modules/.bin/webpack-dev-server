#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules/webpack-dev-server/bin/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules/webpack-dev-server/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules/webpack-dev-server/bin/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules/webpack-dev-server/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/webpack-dev-server@4.15.1_debug@4.3.4_webpack@5.88.1/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
