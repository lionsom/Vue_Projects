#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/cli-service/bin/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/cli-service/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/cli-service/bin/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/cli-service/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules/@vue/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/@vue+cli-service@5.0.8_@babel+core@7.22.6_vue-template-compiler@2.7.14_vue@2.7.14/node_modules:/Users/linxiang/Study/Vue_Projects/尚硅谷-尚品汇/sph/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vue/cli-service/bin/vue-cli-service.js" "$@"
else
  exec node  "$basedir/../@vue/cli-service/bin/vue-cli-service.js" "$@"
fi
